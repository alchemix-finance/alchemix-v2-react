{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" },
    {
      "source": "/vesper-pools/:match*",
      "destination": "https://api.vesper.finance/pools/:match*"
    },
    {
      "source": "/yearn-vaults/:match*",
      "destination": "https://ydaemon.yearn.fi/:match*"
    }
  ]
}
